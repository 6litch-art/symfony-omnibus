{
    "name": "omnibus/omnibus-bundle",
    "type": "symfony-bundle",
    "description": "",
    "keywords": ["symfony"],
    "homepage": "https://symfony.com",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Marco Meyer",
            "email": "marco.meyer@glitchr.io"
        }
    ],
    "autoload": {
